<!--
*  chief details view
*  Uses HTML and embedded JavaScript (EJS) 
*  to dyanmically create a webpage to display a chief.
*
* @author  Jack W Beaver <s526937@nwmissouri.edu>
-->

<% if (chief.error) { %>
    <div class="alert alert-danger" role="alert"><%- chief.error %></div>
    <% } %>
    
    <div class="card-body">
    
      <table class="table">
        <thead>
          <tr>
            <th scope="col">#</th>
            <th scope="col">chief</th>
            <th scope="col">player</th>
            <th scope="col">teamSince</th>
            <th scope="col">isSuperBowlChamp</th>
            <th >Action</th>
          </tr>
        </thead>
        <tbody>
    
          <tr>
            <th scope="row"><%= chief.id %></th>
            <td><%= chief.chief%></td>
            <td><%= chief.player%></td>
            <td><%= chief.teamSince%></td>
            <td><%= chief.isSupwerBowlChamp%></td>
            <td>
              <a class="btn btn-success edit" href="/chief">Done</a>
            </td>
          </tr>
    
        </tbody>
      </table>